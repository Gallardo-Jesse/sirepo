            "ale": ["Z-axis Rotation [rad]", "Float", 0.0, "ALE"]
        },
        "CHANGREF": {
            "name": ["Name", "ValidatedString"],
            "xce": ["Longitudinal Shift [m]", "Float", 0.0, "XCE"],
            "yce": ["Transverse Shift [m]", "Float", 0.0, "YCE"],
            "ale": ["Z-axis Rotation [rad]", "Float", 0.0, "ALE"]
        },
        "DRIFT": {
            "name": ["Name", "ValidatedString"],
            "xl": ["Length [m]", "Float", 0.0, "XL"]
        },
        "MARKER": {
            "name": ["Name", "ValidatedString"],
            "plt": ["Save Particle Data", "Boolean", "1"]
        },
        "OBJET": {
            "name": ["Name", "ValidatedString"],
            "BORO": ["Reference Rigidity [kG/m]", "Float", 0.0, "BORO"],
            "KOBJ": ["Object Format", "ObjetFormat", "5.1", "KOBJ"],
            "dY": ["Sampling size in Y [m]", "Float", 0.0, "dY"],
            "dT": ["Sampling size in T [rad]", "Float", 0.0, "dT"],
            "dZ": ["Sampling size in Z [m]", "Float", 0.0, "dZ"],
            "dP": ["Sampling size in P [rad]", "Float", 0.0, "dP"],
