            "c_4": ["Entrance Frindge Field Coefficients $C_4$", "Float", 0.0],
            "c_5": ["Entrance Frindge Field Coefficients $C_5$", "Float", 0.0],
            "x_s": ["Exit Face Integration Zone Extent [m]", "Float", 0.0],
            "lam_s": ["Exit Fringe Field Extent [m]", "Float", 0.0],
            "w_s": ["Exit Wedge Angle [rad]", "Float", 0.0],
            "cs_0": ["Exit Frindge Field Coefficients $C_0$", "Float", 0.0],
            "cs_1": ["Exit Frindge Field Coefficients $C_1$", "Float", 0.0],
            "cs_2": ["Exit Frindge Field Coefficients $C_2$", "Float", 0.0],
            "cs_3": ["Exit Frindge Field Coefficients $C_3$", "Float", 0.0],
