            "Dprime_y": ["Horizontal Dispersion Derivative [rad]", "Float", 0.0, "$D1_Y$"],
            "D_z": ["Vertical Dispersion [m]", "Float", 0.0, "$D_Z$"],
            "Dprime_z": ["Vertical Dispersion Derivative [rad]", "Float", 0.0, "$D1_Z$"]
        },
        "QUADRUPO": {
            "name": ["Name", "ValidatedString"],
            "xl": ["Length [m]", "Float"],
            "r_0": ["Radius at Pole Tip [m]", "Float", 0.0],
            "b_0": ["Field at Pole Tip [kG]", "Float", 0.0],
            "xpas": ["Integration Step [m]", "Float", 0.0],
            "kpos": ["Alignment", "ElementAlignment", "1"]
        }
