    "strings": {
        "completionState": "",
        "simulationDataType": "notebook",
        "simulationDataTypePlural": "notebooks",
        "typeOfSimulation": "analysis"
    },
    "view": {
        "analysisMetadataReport": {
            "title": "Analysis",
            "advanced": [
		"notes"
	    ]
        },
        "generalMetadataReport": {
            "title": "General",
            "advanced": [
		"notes"
	    ]
