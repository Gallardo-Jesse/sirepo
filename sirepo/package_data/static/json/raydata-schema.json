	    "filename"
	]
    },
    "localRoutes": {
        "analysis": {
            "route": "/analysis/:simulationId",
            "config": {
                "controller": "AnalysisController as analysis",
                "templateUrl": "/static/html/raydata-analysis.html"
          }
        },
        "metadata": {
            "route": "/metadata/:simulationId",
            "config": {
                "controller": "MetadataController as metadata",
                "templateUrl": "/static/html/raydata-metadata.html"
          }
        },
        "dataSource": {
            "route": "/data-source/:simulationId",
            "config": {
                "controller": "DataSourceController as dataSource",
                "templateUrl": "/static/html/raydata-data-source.html"
          }
        }
    },
    "model": {
        "analysisMetadataReport": {
            "notes": ["Notes", "Text", ""]
