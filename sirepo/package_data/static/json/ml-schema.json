        "fileColumnReport": {
            "title": "Column",
            "advanced": []
        },
        "partition": {
            "title": "Partition",
            "basic": [
                "method",
                "training",
                "testing",
                "validation"
            ],
            "advanced": [
                "random_state"
            ]
        },
        "partitionColumnReport": {
            "title": "Column",
            "advanced": []
        },
        "partitionSelectionReport": {
            "title": "Partition Selection",
            "advanced": []
        }
    }
