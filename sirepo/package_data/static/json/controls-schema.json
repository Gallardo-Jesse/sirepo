{
    "appModes": {
        "default": {
            "localRoute": "controls"
        }
    },
    "dynamicFiles": {
        "sirepoLibs": {
            "js": [
                "controls.js",
                "sirepo-lattice.js"
            ],
             "css": [
                 "lattice.css"
            ]
        }
    },
    "enum": {
        "ParticleType": [
            ["positron", "Positron"],
            ["electron", "Electron"],
            ["proton", "Proton"],
            ["antiproton", "Anti-proton"],
            ["posmuon", "Positive Muon"],
            ["negmuon", "Negative Muon"],
            ["ion", "Ion"],
            ["other", "Other"]
        ]
    },
    "localRoutes": {
        "controls": {
            "route": "/controls/:simulationId",
            "config": {
                "controller": "ControlsController as controls",
                "templateUrl": "/static/html/controls-controls.html"
          }
        }
    },
