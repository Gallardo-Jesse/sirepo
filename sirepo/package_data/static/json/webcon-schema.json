{
    "appModes": {
        "default": {
            "localRoute": "analysis"
        }
    },
    "dynamicFiles": {
        "sirepoLibs": {
            "js": ["webcon.js"]
        }
    },
    "localRoutes": {
        "analysis": {
            "route": "/analysis/:simulationId",
            "config": {
                "controller": "AnalysisController as analysis",
                "templateUrl": "/static/html/webcon-analysis.html"
            }
        }
    },
    "enum": {
        "ClusterMethod": [
            ["kmeans", "KMeans"],
            ["gmix", "Gaussian Mixture"],
            ["dbscan", "DBSCAN"],
            ["agglomerative", "Agglomerative Clustering"]
        ],
        "PlotAction": [
            ["cluster", "Cluster"],
            ["fit", "Fit"],
            ["trim", "Trim"]
        ]
    },
    "model": {
        "analysisData": {
            "file": ["Archive Data File", "InputFile"]
        },
        "analysisReport": {
            "x": ["X Value", "AnalysisParameter", "0"],
            "y1": ["Y1 Value", "AnalysisParameter", "1"],
            "action": ["Action", "PlotAction"],
            "notes": ["Notes", "Text", ""],
            "clusterMethod": ["Method", "ClusterMethod", "kmeans"],
            "clusterCount": ["Number of Clusters", "Integer", 3],
            "clusterFields": ["Fields", "ClusterFields"],
            "fitEquation": ["Equation to Fit", "Equation"],
            "fitParameters": ["Fit Parameters", "EquationParameters", ""],
            "fitVariable": ["Independent Variable", "EquationVariables", ""],
            "trimField": ["Trim Field", "AnalysisParameter"],
            "trimMin": ["Minimum", "Float"],
            "trimMax": ["Maximum", "Float"]
        }
    },
    "view": {
        "analysisCluster": {
            "model": "analysisReport",
            "basic": [
                "action",
                "clusterMethod",
                "clusterCount",
                "clusterFields"
            ],
            "advanced": []
        },
        "analysisData": {
            "title": "Archive Data File",
            "basic": [
                "file"
            ],
            "advanced": []
        },
        "analysisFit": {
            "model": "analysisReport",
            "basic": [
                "action",
                "fitEquation",
                "fitVariable",
                "fitParameters"
            ],
            "advanced": []
        },
        "analysisNone": {
            "model": "analysisReport",
            "basic": [
                "action"
            ],
            "advanced": []
        },
        "analysisReport": {
            "title": "Analysis Plot",
            "advanced": [
                "x",
                "y1",
                "notes"
            ]
        },
        "analysisTrim": {
            "model": "analysisReport",
            "basic": [
                "action",
                "trimField",
                "trimMin",
                "trimMax"
            ],
            "advanced": []
        }
    }
}
