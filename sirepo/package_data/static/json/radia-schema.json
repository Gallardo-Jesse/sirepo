{
    "appModes": {
       "default": {
            "localRoute": "visualization"
        }
    },
    "constants": {
        "defaultGeomColor": "#ffffff",
        "inProgressText": "Solving",
        "objectScale": 0.001,
        "pathPtsFileType": "path-pts",
        "pathDetailFields": {
            "circlePath": ["ctrX", "ctrY", "ctrZ", "radius", "theta", "phi"],
            "fieldMapPath": ["ctrX", "ctrY", "ctrZ", "lenX", "lenY", "lenZ"],
            "filePath": ["fileName"],
            "linePath": ["beginX", "beginY", "beginZ", "endX", "endY", "endZ"],
            "manualPath": ["ptX", "ptY", "ptZ"]
        },
        "toolbarItems": [
            {
                "canDrag": true,
                "contents": [
                    {
                        "layoutShape": "rect",
                        "model": "box",
                        "title": "Box",
                        "type": "box"
                    }
                ],
                "type": "Objects"
            },
            {
                "canDrag": false,
                "contents": [
                    {
                        "layoutShape": "rect",
                        "model": "geomGroup",
                        "title": "Add Group",
                        "type": "group"
                    },
                    {
                        "layoutShape": "rect",
                        "model": "undulatorGroup",
                        "title": "Build Undulator",
                        "type": "undulatorGroup"
                    }
                ],
                "type": "Tools"
            }
        ]
    },
    "dynamicFiles": {
        "externalLibs": {
            "js": ["vtk.js"]
        },
        "sirepoLibs": {
            "css": ["vtk.css"],
            "js": ["radia.js"]
        }
    },
    "enum": {
        "BeamAxis": [
            ["x", "x"],
            ["y", "y"],
            ["z", "z"]
        ],
        "DivisionType": [
            ["cyl", "Cylindrical"],
            ["", "None"],
            ["pln", "Planar"]
        ],
        "FieldScaling": [
            ["uniform", "Uniform"],
            ["linear", "Linear"],
            ["log", "Log"]
        ],
        "FieldType": [
            ["M", "M"],
            ["B", "B"],
            ["A", "A"],
            ["H", "H"],
            ["J", "J"]
        ],
        "MaterialType": [
            ["NdFeB", "Neodymium Iron Boron"],
            ["SmCo5", "Samarium Cobalt (SmCo5)"],
            ["Sm2Co17", "Samarium Cobalt (Sm2Co17)"],
            ["Ferrite", "Ferrite"],
            ["Xc06", "Industeel"],
            ["Steel37", "Steel37"],
            ["Steel42", "Steel42"],
            ["AFK502", "Aperam 502"],
            ["AFK1", "Aperam 1"]
        ],
        "ObjectType": [
            ["box", "Box"]
        ],
        "PathType": [
            ["line", "Line"],
            ["circle", "Circle"],
            ["manual", "Manual Group"],
            ["file", "File"],
            ["fieldMap", "Field Map"]
        ],
        "SolveMethod": [
            ["0", "0"],
            ["3", "3"],
            ["4", "4"],
            ["5", "5"]
        ],
        "SymmetryType": [
            ["none", "None"],
            ["parallel", "Parallel"],
            ["perpendicular", "Perpendicular"]
        ],
        "ViewType": [
            ["objects", "Objects"],
            ["fields", "Fields"]
        ]
    },
    "frameIdFields": {
        "solver": [],
        "magnetViewer": []
    },
    "localRoutes": {
        "source": {
            "route": "/source/:simulationId",
            "config": {
                "controller": "RadiaSourceController as src",
                "templateUrl": "/static/html/radia-source.html"
            }
        },
        "visualization": {
            "route": "/visualization/:simulationId",
            "config": {
                "controller": "RadiaVisualizationController as viz",
                "templateUrl": "/static/html/radia-visualization.html"
            }
        }
    },
    "model": {
        "box": {
            "_super": ["_", "model", "geomObject"]
        },
        "circlePath": {
            "_super": ["_", "model", "fieldPath"],
            "ctrX": ["Center x (mm)", "Float", 0.0],
            "ctrY": ["Center y (mm)", "Float", 0.0],
            "ctrZ": ["Center z (mm)", "Float", 0.0],
            "radius": ["Radius", "Float", 10.0],
            "theta": ["\uD835\uDFB1", "Float", 0.0, "Euler 2", -3.14159, 3.14159],
            "phi": ["\uD835\uDFBF", "Float", 0.0, "Euler 3", 0.0, 6.2832],
            "numPoints": ["Num Points", "Integer", 10, "", 2],
            "type": ["_", "PathType", "circle"]
        },
        "fieldDisplay": {
            "colorMap": ["Color Map", "ColorMap", "viridis"],
            "scaling": ["Scaling", "FieldScaling", "uniform"]
        },
        "fieldMapPath": {
            "_super": ["_", "model", "fieldPath"],
            "ctrX": ["Center x (mm)", "Float", 0.0],
            "ctrY": ["Center y (mm)", "Float", 0.0],
            "ctrZ": ["Center z (mm)", "Float", 0.0],
            "lenX": ["X length (mm)", "Float", 1.0, "", 0],
            "lenY": ["Y length (mm)", "Float", 1.0, "", 0],
            "lenZ": ["Z length (mm)", "Float", 1.0, "", 0],
            "numPoints": ["Num Points", "Integer", 10, "", 2],
            "type": ["_", "PathType", "fieldMap"]
        },
        "fieldPath": {
            "name": ["Name", "String", ""]
        },
        "fieldPaths": {
            "path": ["Path", "PathType", "line"]
        },
        "filePath": {
            "_super": ["_", "model", "fieldPath"],
            "fileModel": ["File", "PtsFile", ""],
            "fileName": ["File Name", "String", ""],
            "type": ["_", "PathType", "file"]
        },
        "geometry": {
            "alpha": ["Alpha", "Range", 1.0, "", 0.0, 1.0],
            "beamAxis": ["Beam Axis", "BeamAxis", "z", "The beam will travel along this axis, used when exporting fields"],
            "name": ["Name", "String", ""]
        },
        "geomGroup": {
            "_super": ["_", "model", "radiaObject"],
            "layoutShape":  ["_", "String", "rect"],
            "members": ["_", "Group", []]
        },
        "geomObject": {
            "_super": ["_", "model", "radiaObject"],
            "center": ["Center", "FloatStringArray", "0.0, 0.0, 0.0", "", "x (mm), y (mm), z (mm)"],
            "division": ["Num Divisions", "IntStringArray", "1, 1, 1", "", "Nx, Ny, Nz"],
            "doDivide": ["Divide", "Boolean", "1"],
            "id": ["_", "String", ""],
            "layoutShape":  ["_", "String", "rect"],
            "magnetization": ["Magentization (T)", "FloatStringArray", "0.0, 0.0, 0.0", "", "x, y, z"],
            "mapId": ["_", "String", "???.0"],
            "material": ["Material", "MaterialType", "NdFeB"],
            "type": ["Type", "ObjectType", "box"],
            "size": ["Size", "FloatStringArray", "10.0, 10.0, 10.0", "", "x (mm), y (mm), z (mm)"]
        },
        "geomUndulatorGroup": {
            "_super": ["_", "model", "geomGroup"],
            "baseGroup": [""],
            "gap": ["Gap", "Float", 1.0],
            "numPeriods": ["Number of Periods", "Integer", 1, "", 1]
        },
        "layoutObject": {
            "color": ["Color", "Color", "#cccccc"],
            "groupId": ["_", "String", ""],
            "id": ["_", "String", ""],
            "layoutShape":  ["_", "String", "rect"]
        },
        "linePath": {
            "_super": ["_", "model", "fieldPath"],
            "beginX": ["x1 (mm)", "Float", -10.0],
            "beginY": ["y1 (mm)", "Float", 0.0],
            "beginZ": ["z1 (mm)", "Float", 0.0],
            "begin": ["Start", "FloatStringArray", "-10.0, 0, 0", "Start point", "x, y, z"],
            "end": ["End", "FloatStringArray", "10.0, 0, 0", "End point", "x, y, z"],
            "endX": ["x2 (mm)", "Float", 10.0],
            "endY": ["y2 (mm)", "Float", 0.0],
            "endZ": ["z2 (mm)", "Float", 0.0],
            "numPoints": ["Num Points", "Integer", 10, "", 2],
            "type": ["_", "PathType", "line"]
        },
        "magnet": {
            "_super": ["_", "model", "geometry"]
        },
        "magnetDisplay": {
            "bgColor": ["Background Color", "Color", "#ffffff"],
            "fieldType": ["Field", "FieldType", "M"],
            "notes": ["Notes", "Text", ""],
            "viewType": ["View", "ViewType", "objects"]
        },
        "manualPath": {
            "_super": ["_", "model", "fieldPath"],
            "ptX": ["x (mm)", "Float", 0.0],
            "ptY": ["y (mm)", "Float", 0.0],
            "ptZ": ["z (mm)", "Float", 0.0],
            "type": ["_", "PathType", "manual"]
        },
        "material": {
            "density": ["Density", "Float", 1.0],
            "name": ["Name", "String", ""]
        },
        "radiaObject": {
            "_super": ["_", "model", "layoutObject"],
            "name": ["Name", "String", ""],
            "symmetryPlane": ["Symmetry Plane", "FloatStringArray", "1.0, 0.0, 0.0", "", "x, y, z"],
            "symmetryPoint": ["Point in Symmetry Plane", "FloatStringArray", "0.0, 0.0, 0.0", "", "x, y, z"],
            "symmetryType": ["Symmetry", "SymmetryType", "none"]
        },
        "radiaReq": {
        },
        "reset": {
        },
        "simulation": {
        },
        "solver":  {
            "lastSolved": ["", "Integer", "-1"],
            "maxIterations": ["Max iterations", "Integer", 1500, "", 1],
            "method": ["Method", "SolveMethod", "0"],
            "precision": ["Precision (T)", "Float", 0.00010, "", ""],
            "result": ["Result", "String", ""],
            "sbatchCores": ["Cores", "Integer", 1],
            "sbatchHours": ["Hours", "Float", 0.4],
            "sbatchQueue": ["Queue", "NERSCQueue", "debug"],
            "sbatchProject": ["Project", "OptionalString", ""]
        }
    },
    "view": {
        "circlePath": {
            "title": "Circle",
            "basic": [
                "name",
                "ctrX",
                "ctrY",
                "ctrZ",
                "radius",
                "theta",
                "phi",
                "numPoints"
            ]
        },
        "fieldMapPath": {
            "title": "Field Map",
            "basic": [
                "name",
                "ctrX",
                "ctrY",
                "ctrZ",
                "lenX",
                "lenY",
                "lenZ",
                "numPoints"
            ]
        },
        "fieldPaths": {
            "title": "Field Paths",
            "basic": [
                "path"
            ],
            "advanced": [
                "path"
            ]
        },
        "filePath": {
            "title": "File",
            "basic": [
                "name",
                "fileModel"
            ]
        },
        "geomGroup": {
            "title": "Group",
            "basic": [
                "name",
                "symmetryType",
                "symmetryPlane",
                "symmetryPoint",
                "color",
                "members"
            ],
            "advanced": [
                "name",
                "symmetryType",
                "symmetryPlane",
                "symmetryPoint",
                "color",
                "members"
            ]
        },
        "geomObject": {
            "title": "Object",
            "basic": [
                "name",
                "type",
                "material",
                "center",
                "size",
                "color"
            ],
            "advanced": [
                "name",
                "type",
                "magnetization",
                "material",
                "doDivide",
                "division",
                "symmetryType",
                "symmetryPlane",
                "symmetryPoint",
                "center",
                "size",
                "color"
            ]
        },
        "linePath": {
            "title": "Line",
            "basic": [
                "name",
                "begin",
                "beginX",
                "beginY",
                "beginZ",
                "end",
                "endX",
                "endY",
                "endZ",
                "numPoints"
            ]
        },
        "magnetDisplay": {
            "title": "Magnet Viewer",
            "basic": [
                "geometry.name",
                "geometry.beamAxis",
                "viewType",
                "bgColor",
                "geometry.alpha",
                "fieldType",
                "fieldDisplay.colorMap",
                "fieldDisplay.scaling"
            ],
            "advanced": [
                "notes"
            ]
        },
        "manualPath": {
            "title": "Manual",
            "basic": [
                "name",
                "ptX",
                "ptY",
                "ptZ"
            ]
        },
        "simulation": {
            "title": "Simulation",
            "advanced": [
                "name",
                "folder",
                "geometry.beamAxis"
            ]
        },
        "solver": {
            "title": "Solver",
            "basic": [
                "precision",
                "maxIterations",
                "method"
            ],
            "advanced": []
        }
    }
}
